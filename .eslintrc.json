{
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": [
      "plugin:react/recommended",
      "airbnb"
    ],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "plugins": [
      "react"
    ],
    "rules": {
      "radix": "off",
      "no-shadow": "off",
      "max-len": "off",
      "quote-props": "off",
     " react/jsx-props-no-spreading": "off",
      "no-underscore-dangle": "off",
      "semi": ["error", "always"],
      "no-console": "off",
      "react/prop-types": "off",
      // "jsx-a11y/label-has-for": "off",
      "jsx-a11y/label-has-associated-control": "off"
    }
  }